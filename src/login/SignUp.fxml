<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="320.0" prefWidth="420.0" stylesheets="@Signup.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="login.SignUpController">
   <children>
      <Label layoutX="150.0" layoutY="20.0" text="Sign up">
         <font>
            <Font name="Berlin Sans FB" size="40.0" />
         </font>
      </Label>
      <Label layoutX="30.0" layoutY="127.0" text="아이디" />
      <Label layoutX="30.0" layoutY="170.0" text="비밀번호" />
      <Label layoutX="30.0" layoutY="216.0" text="비밀번호 확인" />
      <Label layoutX="30.0" layoutY="85.0" text="닉네임" />
      <TextField fx:id="txtName" layoutX="120.0" layoutY="80.0" prefHeight="23.0" prefWidth="184.0" />
      <TextField fx:id="txtId" layoutX="120.0" layoutY="124.0" prefHeight="23.0" prefWidth="184.0" promptText="한글, 특수문자, 공백 사용불가" />
      <Button fx:id="btnCheck" layoutX="321.0" layoutY="123.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="76.0" text="중복검사" />
      <PasswordField fx:id="txtPw" layoutX="120.0" layoutY="168.0" prefHeight="23.0" prefWidth="184.0" />
      <PasswordField fx:id="txtPw2" layoutX="120.0" layoutY="212.0" prefHeight="23.0" prefWidth="184.0" />
      <Button fx:id="btnOk" layoutX="119.0" layoutY="268.0" mnemonicParsing="false" text="회원가입 완료" />
      <Button fx:id="btnCancel" layoutX="238.0" layoutY="268.0" mnemonicParsing="false" text="취소" />
   </children>
</AnchorPane>
